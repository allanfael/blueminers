{
  "extends": "expo/tsconfig.base",
  "jsx": "react-jsx",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "allowJs": false,
    "sourceMap": true,
    "noEmit": true,
    "noImplicitReturns": true,
    "baseUrl": "./src",
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "@components/*": [
        "./components/*"
      ],
       "@components": [
        "./components"
      ],
      "@mock/*": [
        "./mock/*"
      ],
       "@mock": [
        "./mock"
      ],
      "@helpers/*": [
        ".utils/helpers/*"
      ],
      "@helpers": [
        ".utils/helpers"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@screens": [
        "./screens"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@services": [
        "./services"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@store": [
        "./store"
      ],
      "@themes/*": [
        "./themes/*"
      ],
      "@themes": [
        "./themes"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@assets": [
        "./assets"
      ],
      "@config/*": [
        "./config/*"
      ],
      "@config": [
        "./config"
      ],
      "@navigator/*": [
        "./navigators/*"
      ],
       "@navigator": [
        "./navigators"
      ],
      "@shimmers/*": [
        "./shimmers/*"
      ],
      "@shimmers": [
        "./shimmers"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@hooks": [
        "./hooks"
      ],
      "@types/*": [
        "./utils/types/*"
      ],
      "@types": [
        "./utils/types"
      ],
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
