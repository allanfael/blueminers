{
  "expo": {
    "name": "BluMiners",
    "slug": "bluminers",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fafafa",
      "dark": {
        "image": "./assets/dark-splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.allanfael.bluminers"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.allanfael.bluminers"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": false
    },
    "build": {
      "preview": {
        "distribution": "internal"
      },
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      }
    },
    "extra": {
      "eas": {
        "projectId": "dbe08444-81c9-4331-9d21-1ca620ca4827"
      }
    }
  }
}